{
  "name": "ffan/dop-uis",
  "description": "Dop ui service framework",
  "keywords": [
    "ffan",
    "dop",
    "uis"],
  "type": "composer",
  "license": "BSD-3-Clause",
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6.0",
    "ffan/std-event": "2.*",
    "ffan/std-common": "2.*",
    "ffan/std-logger": "1.*",
    "ffan/std-cache": "2.*",
    "ffan/std-console": "1.*",
    "ffan/php-dop": "2.*",
    "ffan/std-async": "1.*",
    "ffan/std-http": "1.*",
    "ffan/std-tpl": "1.*"
  },
  "autoload": {
    "psr-4": {
      "FFan\\Dop\\Uis\\": "src",
      "FFan\\Uis\\Work\\": "work"
    }
  },
  "config": {
    "secure-http": false
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://composer.intra.ffan.com/repo/private/"
    },
    {
      "type": "composer",
      "url": "http://composer.intra.ffan.com/repo/packagist/"
    },
    {
      "packagist": false
    }
  ]
}
